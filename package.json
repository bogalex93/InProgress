{
  "name": "inprogress",
  "version": "0.0.0",
  "author": "Bogdan Alexandru",
  "description": "Personal organizer and task management",
  "main": "prod_build/electron/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "app-compile": "ng build --base-href ./ && tsc --p electron",
    "electron-assets": "cpy ./data ./_dist/data",
    "electron-start": "tsc --p electron && electron .",
    "electron-compile": "npm run electron-assets && tsc --p electron",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "build-prod": "npm run app-compile  &&  electron-builder -w",
    "build:snap": "electron-installer-snap --src=prod/InProgress-linux-x64"
  },
  "build": {
    "appId": "Video-Projector",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {}
  },
  "private": true,
  "dependencies": {
    "@angular/common": "16.1.3",
    "@angular/compiler": "16.1.3",
    "@angular/core": "16.1.3",
    "@angular/forms": "16.1.3",
    "@angular/language-service": "16.1.3",
    "@angular/platform-browser": "16.1.3",
    "@angular/platform-browser-dynamic": "16.1.3",
    "@angular/router": "16.1.3",
    "cross-blob": "^2.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "knex": "3.1.0",
    "lodash": "^4.17.20",
    "path-browserify": "^1.0.1",
    "rxjs": "7.8.1",
    "sqlite3": "^5.1.6",
    "tslib": "^2.5.0",
    "uikit": "^3.4.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "16.0.0",
    "@angular-devkit/build-angular": "16.1.3",
    "@angular/cli": "16.1.3",
    "@angular/compiler-cli": "16.1.3",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@types/jquery": "^3.5.5",
    "@types/jqueryui": "^1.12.14",
    "@types/lodash": "^4.14.156",
    "@types/node": "18.16.9",
    "cpy-cli": "^5.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-installer-snap": "^5.1.0",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9",
    "node-polyfill-webpack-plugin": "2.0.1",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5",
    "webdriver-manager": "12.1.9"
  },
  "engines": {
    "node": ">= 16.14.0 || >= 18.10.0"
  }
}
