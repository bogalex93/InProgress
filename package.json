{
  "name": "in-progress",
  "version": "0.0.0",
  "author": "Rom Dev Bogdan T. Alexandru",
  "description": "Personal organizer and task management",
  "main": "_dist/electron/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "app-compile": "ng build --base-href ./ && tsc --p electron",
    "electron-start": "tsc --p electron && electron .",
    "electron-compile": "tsc --p electron",
    "electron-build": "npm run app-compile && electron-builder -w"
  },
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "cross-blob": "^2.0.0",
    "crypto-js": "^4.0.0",
    "font-awesome": "^4.7.0",
    "glasstron": "^0.1.1",
    "js-guid": "^1.0.0",
    "knex": "~0.21.17",
    "lodash": "^4.17.20",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/node": "~16.11.4",
    "electron": "^15.3.0",
    "electron-builder": "^22.9.1",
    "electron-installer-snap": "^5.1.0",
    "electron-packager": "^15.1.0",
    "tslib": "^2.0.0",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">=12.00.0"
  }
}
