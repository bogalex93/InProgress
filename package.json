  "description": "Personal organizer and task management",
  "main": "prod_build/electron/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "app-compile": "ng build --base-href ./ && tsc --p electron",
